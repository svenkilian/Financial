{
  "data": {
    "filename": "test.csv",
    "_comment": "['Close','Volume']",
    "columns": [
      "Close",
      "Volume"
    ],
    "sequence_length": 241,
    "train_test_split": 0.75,
    "normalize": true
  },
  "training": {
    "epochs": 1000,
    "batch_size": 26,
    "early_stopping_patience": 10
  },
  "model": {
    "loss": "binary_crossentropy",
    "optimizer": "rmsprop",
    "optimizer_params": {
    },
    "metrics": [
      "accuracy"
    ],
    "save_dir": "saved_models",
    "layers": [
      {
        "type": "conv1d",
        "params":
        {
          "filters": 5,
          "kernel_size": 4,
          "strides": 2,
          "padding": "valid",
          "input_shape": [null, 1]
        }
      },
      {
        "type": "gru",
        "params": {
          "units": 50,

          "dropout": 0.1,
          "recurrent_dropout": 0.0,
          "kernel_regularizer": null,
          "return_sequences": false
        }
      },
      {
        "type": "dense",
        "params": {
          "units": 1,
          "activation": "sigmoid"
        }
      }
    ]
  }
}
